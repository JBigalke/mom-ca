    <xrx:xml>
        <cei:text xmlns:cei="http://www.monasterium.net/NS/cei">
            <cei:front>
                <cei:sourceDescVolltext>
                    <cei:bibl>simple-input</cei:bibl>
                </cei:sourceDescVolltext>
                <cei:sourceDescRegest>
                    <cei:bibl>simple-input</cei:bibl>
                </cei:sourceDescRegest>
            </cei:front>
            <cei:body>
                <cei:chDesc>
                    <cei:abstract>mixed-textarea</cei:abstract>
                    <cei:issued>
                        <cei:placeName>mixed-textarea</cei:placeName>
                        <cei:dateRange from="" to="">simple-input</cei:dateRange>
                        <cei:date value="">simple-input</cei:date>
                    </cei:issued>
                    <cei:witnessOrig>
                        <cei:figure>
                            <cei:graphic url="">empty</cei:graphic>
                        </cei:figure>
                        <cei:traditioForm>simple-input</cei:traditioForm>
                        <cei:archIdentifier>mixed-textarea</cei:archIdentifier>
                        <cei:physicalDesc>
                            <cei:material>mixed-textarea</cei:material>
                            <cei:dimensions>mixed-textarea</cei:dimensions>
                            <cei:condition>mixed-textarea</cei:condition>
                            <cei:p>mixed-textarea</cei:p>
                        </cei:physicalDesc>
                        <cei:auth>
                            <cei:sealDesc>mixed-textarea</cei:sealDesc>
                            <cei:notariusDesc>mixed-textarea</cei:notariusDesc>
                        </cei:auth>
                        <cei:nota>mixed-textarea</cei:nota>
                    </cei:witnessOrig>
                    <cei:witListPar>
                        <cei:witness>
                            <cei:traditioForm>simple-input</cei:traditioForm>
                            <cei:archIdentifier>mixed-textarea</cei:archIdentifier>
                            <cei:physicalDesc>
                                <cei:material>simple-input</cei:material>
                                <cei:dimensions>mixed-textarea</cei:dimensions>
                                <cei:condition>mixed-textarea</cei:condition>
                                <cei:p>mixed-textarea</cei:p>
                            </cei:physicalDesc>
                            <cei:auth>
                                <cei:sealDesc>mixed-textarea</cei:sealDesc>
                                <cei:notariusDesc>mixed-textarea</cei:notariusDesc>
                            </cei:auth>
                            <cei:nota>mixed-textarea</cei:nota>
                        </cei:witness>
                    </cei:witListPar>
                    <cei:diplomaticAnalysis>
                        <cei:listBibl>
                            <cei:bibl>simple-input</cei:bibl>
                        </cei:listBibl>
                        <cei:listBiblEdition>
                            <cei:bibl>simple-input</cei:bibl>
                        </cei:listBiblEdition>
                        <cei:listBiblRegest>
                            <cei:bibl>simple-input</cei:bibl>
                        </cei:listBiblRegest>
                        <cei:listBiblFaksimile>
                            <cei:bibl>simple-input</cei:bibl>
                        </cei:listBiblFaksimile>
                        <cei:listBiblErw>
                            <cei:bibl>simple-input</cei:bibl>
                        </cei:listBiblErw>
                        <cei:quoteOrginaldatierung>simple-input</cei:quoteOrginaldatierung>
                        <cei:nota type="">mixed-textarea</cei:nota>
                    </cei:diplomaticAnalysis>
                    <cei:lang_MOM>simple-input</cei:lang_MOM>
                </cei:chDesc>
                <cei:tenor>mixed-textarea</cei:tenor>
            </cei:body>
            <cei:back>
                <cei:persName type="">mixed-textarea</cei:persName>
                <cei:placeName type="">mixed-textarea</cei:placeName>
                <cei:index type="">mixed-textarea</cei:index>
                <cei:divNotes>
                    <cei:note n="">mixed-textarea</cei:note>
                </cei:divNotes>
